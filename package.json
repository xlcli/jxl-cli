	},
	"dependencies": {
		"@octokit/core": "^3.1.0",
		"archiver": "^4.0.1",
		"chalk": "^4.0.0",
		"commander": "^5.1.0",
		"cross-spawn": "^7.0.3",
		"download-git-repo": "^3.0.2",
